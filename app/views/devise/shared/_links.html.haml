- if controller_name != 'sessions'
  = link_to t('devise.sessions.form_title'), new_session_path(resource_name)
  %br
- if devise_mapping.registerable? && controller_name != 'registrations'
  = link_to t('devise.registrations.form_title'), new_registration_path(resource_name)
  %br
- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations'
  = link_to t('devise.passwords.forgot_your_password'), new_password_path(resource_name)
